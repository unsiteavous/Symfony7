#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/media/acherontia/nextcloud/FORMATIONS/Simplon/2024/TRAVAUX/14 Symfony/node_modules/.pnpm/maildev@2.2.1/node_modules/maildev/bin/node_modules:/media/acherontia/nextcloud/FORMATIONS/Simplon/2024/TRAVAUX/14 Symfony/node_modules/.pnpm/maildev@2.2.1/node_modules/maildev/node_modules:/media/acherontia/nextcloud/FORMATIONS/Simplon/2024/TRAVAUX/14 Symfony/node_modules/.pnpm/maildev@2.2.1/node_modules:/media/acherontia/nextcloud/FORMATIONS/Simplon/2024/TRAVAUX/14 Symfony/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/media/acherontia/nextcloud/FORMATIONS/Simplon/2024/TRAVAUX/14 Symfony/node_modules/.pnpm/maildev@2.2.1/node_modules/maildev/bin/node_modules:/media/acherontia/nextcloud/FORMATIONS/Simplon/2024/TRAVAUX/14 Symfony/node_modules/.pnpm/maildev@2.2.1/node_modules/maildev/node_modules:/media/acherontia/nextcloud/FORMATIONS/Simplon/2024/TRAVAUX/14 Symfony/node_modules/.pnpm/maildev@2.2.1/node_modules:/media/acherontia/nextcloud/FORMATIONS/Simplon/2024/TRAVAUX/14 Symfony/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/maildev" "$@"
else
  exec node  "$basedir/../../bin/maildev" "$@"
fi
